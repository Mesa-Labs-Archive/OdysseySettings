<resources>

    <!-- Base application theme. -->
    <!-- Light -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#2196F3</item>
        <item name="tabs_text_color">@color/tabs_text_color_dark</item>
        <item name="tabs_indicator_color">#ffffff</item>
        <item name="colorPrimaryDark">#1E88E5</item>
        <item name="colorAccent">#42A5F5</item>
        <item name="sublimetheme">@style/SublimeTheme</item>
        <item name="android:dialogTheme">@style/GrxDialogStyle</item>
        <item name="android:alertDialogTheme">@style/GrxDialogStyle</item>
        <item name="svn_nav_header_bg">?colorPrimary</item>
        <item name="navigationviewstyle">@style/NavigationViewStyle</item>
        <item name="fab_m_color">?colorAccent</item>
        <item name="fab_m_press">?colorPrimaryDark</item>
        <item name="fab_m_ripple">?colorPrimary</item>
        <item name="main_bg_color">#ffffffff</item>
        <item name="pickerColorEditTextColor">0xff222222</item>
        <item name="tint_arrow_prefs">#ff222222</item>
        <item name="pref_category_text_color">?colorPrimary</item>
        <item name="pref_category_bg">#00000000</item>
        <item name="seekbar_popup_bg">?colorAccent</item>
        <item name="snackbar_bg">#304553</item>
        <item name="contrast_text_color">#ffffff</item>
		<item name="prefscreen_arrow_tint">?colorAccent</item>
        <item name="complemnt_accent_color">?colorAccent</item>
    </style>

    <style name="SublimeTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorPrimary">#2196F3</item>
        <item name="colorPrimaryDark">#1E88E5</item>
        <item name="colorAccent">#1E88E5</item>
        <item name="android:listDivider">@color/divider_svn</item>
        <item name="svn_panel_bg">#fffafafa</item>
        <item name="navigationviewstyle">@style/NavigationViewStyle</item>
		<item name="svn_fab_tint">?colorAccent</item>
    </style>
	
	

    <style name="NavigationViewStyle" parent="SnvSublimeNavigationView">
        <item name="android:maxWidth">@dimen/snv_navigation_max_width</item>

    </style>



    <style name="GrxDialogStyle">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@style/GrxTitleStyle</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@drawable/dialog_background_material</item>
        <item name="android:windowMinWidthMajor">70.0%</item>
        <item name="android:windowMinWidthMinor">90.0%</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="android:windowNoTitle">false</item>
    </style>


    <!-- Dark -->

    <style name="Theme.Base.Dark" parent="Theme.AppCompat">
        <item name="tabs_text_color">@color/tabs_text_color_dark</item>
        <item name="tabs_indicator_color">#ffffff</item>
        <item name="sublimetheme">@style/SublimeTheme_dark</item>
        <item name="colorPrimaryDark">#ff6600ff</item>
        <item name="colorPrimary">#ff6600ff</item>
        <item name="colorAccent">#ff6600ff</item>
        <item name="tint_arrow_prefs">#ffffffff</item>
        <item name="android:dialogTheme">@style/GrxDialogStyle</item>
        <item name="android:alertDialogTheme">@style/GrxDialogStyle</item>
        <item name="svn_nav_header_bg">?colorPrimary</item>
        <item name="navigationviewstyle">@style/NavigationViewStyle</item>
        <item name="fab_m_color">?colorAccent</item>
        <item name="fab_m_press">?colorPrimaryDark</item>
        <item name="fab_m_ripple">?colorPrimary</item>
        <item name="main_bg_color">#ff1a1a1a</item>
        <item name="pickerColorEditTextColor">0xffffffff</item>
        <item name="pref_category_text_color">?colorPrimaryDark</item>
        <item name="pref_category_bg">#00000000</item>
        <item name="seekbar_popup_bg">?colorAccent</item>
        <item name="snackbar_bg">#6600ff</item>
		<item name="prefscreen_arrow_tint">?colorAccent</item>
        <item name="complemnt_accent_color">?colorAccent</item>
        <item name="contrast_text_color">#222222</item>
    </style>


    <style name="SublimeTheme_dark" parent="ThemeOverlay.AppCompat.Dark">
        <item name="colorPrimary">#ff6600ff</item>
        <item name="colorPrimaryDark">#ff6600ff</item>
        <item name="colorAccent">#ff6600ff</item>
        <item name="svn_panel_bg">#ff1a1a1a</item>
        <item name="android:listDivider">@color/divider_svn</item>
        <item name="navigationviewstyle">@style/NavigationViewStyle</item>
		<item name="svn_fab_tint">?colorAccent</item>
    </style>


    <style name="Theme.Blue.Dark" parent="@style/Theme.Base.Dark">
      <item name="colorPrimary">#ff0081d8</item>
        <item name="colorPrimaryDark">#ff0081d8</item>
        <item name="colorAccent">#0081d8</item>
		<item name="svn_nav_header_bg">?colorPrimary</item>
		<item name="pref_category_text_color">?colorAccent</item>
		<item name="sublimetheme">@style/SublimeTheme_blue_dark</item>
		<item name="snackbar_bg">#ff0081d8</item>

	</style>
	
	 <style name="SublimeTheme_blue_dark" parent="@style/SublimeTheme_dark">
        <item name="colorPrimary">#ff0081d8</item>
        <item name="colorPrimaryDark">#0081d8</item>
        <item name="colorAccent">#0081d8</item>
    </style>


    <style name="Theme.Red.Dark" parent="@style/Theme.Base.Dark" >
		<item name="colorPrimary">#ffdd0000</item>
        <item name="colorPrimaryDark">#ffdd0000</item>
        <item name="colorAccent">#ffdd0000</item>
		<item name="svn_nav_header_bg">?colorPrimary</item>
		<item name="pref_category_text_color">?colorAccent</item>
		<item name="snackbar_bg">#ffdd0000</item>
		<item name="sublimetheme">@style/SublimeTheme_red_dark</item>
        <item name="complemnt_accent_color">#ffffff</item>
	</style>
	
    <style name="SublimeTheme_red_dark" parent="@style/SublimeTheme_dark">
        <item name="colorPrimary">#ffdd0000</item>
        <item name="colorPrimaryDark">#ffdd0000</item>
        <item name="colorAccent">#ffdd0000</item>
    </style>


    <style name="Theme.Yellow.Dark" parent="@style/Theme.Base.Dark">
      <item name="colorPrimary">#ffe6f400</item>
        <item name="colorPrimaryDark">#ffe6f400</item>
        <item name="colorAccent">#ffe6f400</item>
		<item name="svn_nav_header_bg">?colorPrimary</item>
		<item name="pref_category_text_color">?colorAccent</item>
		<item name="sublimetheme">@style/SublimeTheme_yellow_dark</item>
		<item name="snackbar_bg">#ffe6f400</item>
        <item name="fab_m_color">?colorPrimaryDark</item>
        <item name="fab_m_press">?colorAccent</item>
        <item name="fab_m_ripple">?colorAccent</item>
        <item name="seekbar_popup_bg">#e6f400</item>
        <item name="contrast_text_color">#e6f400</item>

	</style>
	
	 <style name="SublimeTheme_yellow_dark" parent="@style/SublimeTheme_dark">
        <item name="colorPrimary">#ffe6f400</item>
        <item name="colorPrimaryDark">#ffe6f400</item>
        <item name="colorAccent">#ffe6f400</item>
		<item name="svn_fab_tint">#e6f400</item>
    </style>


    <style name="Theme.Orange.Dark" parent="@style/Theme.Base.Dark">
      <item name="colorPrimary">#dd6f00</item>
        <item name="colorPrimaryDark">#dd6f00</item>
        <item name="colorAccent">#dd6f00</item>
		<item name="svn_nav_header_bg">?colorPrimary</item>
		<item name="pref_category_text_color">?colorAccent</item>
		<item name="sublimetheme">@style/SublimeTheme_orange_dark</item>
		<item name="snackbar_bg">#dd6f00</item>
        <item name="fab_m_color">?colorPrimaryDark</item>
        <item name="fab_m_press">?colorAccent</item>
        <item name="fab_m_ripple">?colorAccent</item>
	</style>
	
	 <style name="SublimeTheme_orange_dark" parent="@style/SublimeTheme_dark">
      <item name="colorPrimary">#dd6f00</item>
        <item name="colorPrimaryDark">#dd6f00</item>
        <item name="colorAccent">#dd6f00</item>
		<item name="svn_fab_tint">#dd6f00</item>
    </style>	


    <style name="Theme.Green.Dark" parent="@style/Theme.Base.Dark">
      <item name="colorPrimary">#ff00d83a</item>
        <item name="colorPrimaryDark">#ff00d83a</item>
        <item name="colorAccent">#00d83a</item>
		<item name="svn_nav_header_bg">?colorPrimary</item>
		<item name="pref_category_text_color">?colorAccent</item>
		<item name="sublimetheme">@style/SublimeTheme_green_dark</item>
		<item name="snackbar_bg">#ff00d83a</item>

	</style>
	
	 <style name="SublimeTheme_green_dark" parent="@style/SublimeTheme_dark">
        <item name="colorPrimary">#ff00d83a</item>
        <item name="colorPrimaryDark">#00d83a</item>
        <item name="colorAccent">#00d83a</item>
    </style>
	
	
    <!-- common -->


    <style name="GrxTitleStyle" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Title" >
        <item name="android:textSize">17dp</item>
        <item name="android:textColor">?colorAccent</item>
    </style>

    <style name="ToolbarTitleAppearance" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">17dp</item>
        <item name="android:textColor">@color/toolbartitlecolor</item>
     </style>

    <style name="ToolbarSubtitleAppearance" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Subtitle">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/toolbarsubtitlecolor</item>
    </style>




    <style name="Grx.NoActionBar" parent="AppTheme.NoActionBar">

    </style>


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />



    <style name="ToolbarTheme" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <!--<item name="colorControlNormal">#FFDDD3</item>-->
        <item name="android:textColorPrimary">#FFFFFF</item>
    </style>
    <style name="PickerEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="AppTheme.Toolbar.Title" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="AppTheme.Toolbar.SubTitle" parent="TextAppearance.Widget.AppCompat.Toolbar.Subtitle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#fffdfdfd</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="grx_st_item_spinner">
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>

        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:textColor">?colorAccent</item>

    </style>




</resources>
